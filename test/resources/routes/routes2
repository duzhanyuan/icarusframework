
// Line comment

/**
 * Multiline comment
 */

GET			/									controllers::Index::default0(@client, @session)
GET			/match/<param1:[0-9]+>				controllers::Index::default1(unsigned int param1)
POST		/<count>							controllers::Index::default2(uint8_t count)
POST		/<count>/<id>						controllers::Index::default3(unsigned int count, string id)
PUT			/testing/<count>/<id>/test			controllers::Index::default4(uint8_t id, string count)
PUT			/testing/<count>x<id>test			controllers::Index::default5(unsigned int count, string id)
